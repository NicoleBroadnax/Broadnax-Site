#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/autoprefixer/bin/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/autoprefixer/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules:/Users/nicolebroadnax/Documents/Codes2024/node_modules:/Users/nicolebroadnax/Documents/node_modules:/Users/nicolebroadnax/node_modules:/Users/node_modules:/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/autoprefixer/bin/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/autoprefixer/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules:/Users/nicolebroadnax/Documents/Codes2024/node_modules:/Users/nicolebroadnax/Documents/node_modules:/Users/nicolebroadnax/node_modules:/Users/node_modules:/node_modules:/Users/nicolebroadnax/Documents/Codes2024/broadnax-site/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
